generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Historico {
  id             Int      @id @default(autoincrement())
  idUsuario      Int
  nomeRelatorio  String
  dataGerado     DateTime @default(now())
  tipoRelatorio  String
}
